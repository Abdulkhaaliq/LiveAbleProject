<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="http://prismlibrary.com"
                  xmlns:behaviors="clr-namespace:Prism.Behaviors;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="LiveAble.Views.MainPageMasterDetail">
    <MasterDetailPage.Master>
        <NavigationPage Title="Menu" Icon="hamburger.png">
            <x:Arguments>
                <ContentPage Title="Menu">
                    <StackLayout Padding="10">
                    
                        <Button Text="About Us" Command="{Binding AboutUsCommand}" CommandParameter="NavigationPage/AboutUs" BorderColor="Black" TextColor="Black" HeightRequest="50" CornerRadius="50"  />
                        <Button Text="Login Here" Command="{Binding LoginPageCommand}" CommandParameter="NavigationPage/LoginPage" BorderColor="Black" TextColor="Black" HeightRequest="50" CornerRadius="50"  />

                    </StackLayout>
                </ContentPage>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Master>
</MasterDetailPage>


